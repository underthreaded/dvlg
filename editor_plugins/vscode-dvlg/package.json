{
  "name": "dvlg",
  "repository": {
    "type": "git",
    "url": "https://github.com/underthreaded/dvlg.git"
  },
  "publisher": "underthreaded",
  "displayName": "dvlg",
  "license": "MIT",
  "description": "Support for the dvlg note taking file format",
  "icon": "dvlg.png",
  "galleryBanner": {
    "color": "#0B1F39",
    "theme": "dark"
  },
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.92.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "dvlg",
        "aliases": [
          "dvlg"
        ],
        "extensions": [
          ".dvlg"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "dvlg",
        "scopeName": "source.dvlg",
        "path": "./syntax.json"
      }
    ],
    "snippets": [
      {
        "language": "dvlg",
        "path": "./snippets.json"
      }
    ]
  }
}